ATOI

#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void main () {
   int val;
   char str[20];
   
   strcpy(str, "98993489");
   val = atoi(str);
   printf("String value = %s, Int value = %d\n", str, val);

   strcpy(str, "harsha");
   val = atoi(str);
   printf("String value = %s, Int value = %d\n", str, val);

  
}

