#include<stdio.h>
#define true 1
int main() {
    
   printf("%d",true);
}

----------------------------------------------------------
sum of first 10 numbers

#include<stdio.h>

int main() {
    int i,sum=0;
    for (i=1;i<=10;i++)
    {
    sum+=i;
    }
   printf("%d ",sum);
}

--------------------------------------------------------------------

display of first 10 numbers

#include<stdio.h>

int main() {
    int i;
    for (i=1;i<=10;i++)
   
   printf("%d ",i);
}

----------------------------------------------------------------------------
avg and sum of any 10 numbers

#include<stdio.h>

int main() {
    int a[10],i,sum=0;
    float p;
    for (i=0;i<=9;i++)
    {
         printf("Number-%d: \n",i+1);
        scanf("%d",&a[i]);
        sum+=a[i];
    }
    p=sum/10;
  
    printf("The sum of 10 no is :%d \n",sum);
    printf("The Average is :%f \n",p);
  
}

------------------------------------------------------------------------

cube of a number

#include<stdio.h>

int main() {
    int a[5],i,cube=0;
    for (i=0;i<=4;i++)
    {
         
        scanf("%d",&a[i]);
        cube=(a[i])*(a[i])*(a[i]);
        printf("Number is : %d and cube of the %d is : %d \n",a[i],a[i],cube);
        
    }
    
}


