#include<stdio.h>
#define true 1
int main() {
    
   printf("%d",true);
}

----------------------------------------------------------
sum of first 10 numbers

#include<stdio.h>

int main() {
    int i,sum=0;
    for (i=1;i<=10;i++)
    {
    sum+=i;
    }
   printf("%d ",sum);
}

--------------------------------------------------------------------

display of first 10 numbers

#include<stdio.h>

int main() {
    int i;
    for (i=1;i<=10;i++)
   
   printf("%d ",i);
}

----------------------------------------------------------------------------
avg and sum of any 10 numbers

#include<stdio.h>

int main() {
    int a[10],i,sum=0;
    float p;
    for (i=0;i<=9;i++)
    {
         printf("Number-%d: \n",i+1);
        scanf("%d",&a[i]);
        sum+=a[i];
    }
    p=sum/10;
  
    printf("The sum of 10 no is :%d \n",sum);
    printf("The Average is :%f \n",p);
  
}

------------------------------------------------------------------------

cube of a number

#include<stdio.h>

int main() {
    int a[5],i,cube=0;
    for (i=0;i<=4;i++)
    {
         
        scanf("%d",&a[i]);
        cube=(a[i])*(a[i])*(a[i]);
        printf("Number is : %d and cube of the %d is : %d \n",a[i],a[i],cube);
        
    }
    
}


---------------------------------------------------------

pattern

#include<stdio.h>

int main() {
    int i,j,r;
    scanf("%d",&r);
    for (i=1;i<=r;i++)
    {
       for(j=1;j<=i;j++)
       {
           printf("%d ",i);
           
       }
        printf("\n"); 
    }
    
}

------------------------------------------------------------------























---------------------------------------------------------

numbers equal r not

#include<stdio.h>

int main() {
    int a,b;
    scanf("%d%d",&a,&b);
    if(a==b)
    {
        printf("Number1 and Number2 are equal");
    }
    else
    {
        printf("Number1 and Number2 are not equal");
    }
}

-----------------------------------------------------------------------

positive r negative

#include<stdio.h>

int main() {
    int a;
    scanf("%d",&a);
    if(a>=0)
    {
        printf("%d is a positive number",a);
    }
    else
    {
        printf("%d is a negative number",a);
    }
}

--------------------------------------------------------------------

triangle

#include<stdio.h>

int main() {
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if((a==b)&&(b==c))
    {
        printf("This is an equilateral triangle.");
    }
    else if((a==b)||(b==c)||(c==a))
    {
        printf("This is an isosceles triangle");
    }
    else
    {
        printf("This is an Scalene triangle");
    }
}

---------------------------------------------------------------

alphabet r digit r special character

#include<stdio.h>

void main() {
    char a;
    scanf("%c",&a);
    if((a>='a' && a<='z') || (a>='A' && a<='Z'))
    {
        printf("This is a alphabet");
    }
   else if ((a>='0' && a<='9'))
   printf("This is a digit");
   else
   printf("This is a special character");
    
}
